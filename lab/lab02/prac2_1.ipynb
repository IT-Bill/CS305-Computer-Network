    "def generate(n):\n",
    "    num = 2023\n",
    "    new_digit = None\n",
    "    for i in range(n):\n",
    "        new_digit = ((num % 10) * ((num // 10) % 10)) % 10\n",
    "        num = num * 10 + new_digit\n",
    "    \n",
    "    return new_digit"
   ]
